.editing
  %input.title{type: "text", placeholder: t('placeholders.builder.title'), autofocus: true}

%a.back
  %svg.prefix
    %use{"xlink:href" => "#backward_symbol"}
  %span.label
    = t('buttons.builder.back')

%a.continue{href: "asset"}
  %span.label
    = t('buttons.builder.continue')
  %svg.suffix
    %use{"xlink:href" => "#forward_symbol"}
